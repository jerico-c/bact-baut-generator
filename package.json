{
  "name": "bact-baut-generator",
  "version": "1.0.0",
  "description": "Aplikasi web untuk generate dokumen BACT dan BAUT",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg . --targets node18-win-x64,node18-macos-x64 --output dist/generator"
  },
  "keywords": [
    "bact",
    "baut",
    "generator",
    "docx"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "docxtemplater": "^3.66.4",
    "docxtemplater-image-module-free": "^1.1.1",
    "express": "^5.1.0",
    "image-size": "^2.0.2",
    "multer": "^2.0.2",
    "pizzip": "^3.2.0"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "templates/**/*"
    ],
    "outputPath": "dist"
  }
}
